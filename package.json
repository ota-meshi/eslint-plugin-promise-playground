{
	"name": "eslint-plugin-promise-playground",
	"version": "1.0.0",
	"description": "eslint-plugin-promise Online Playground",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"open-site": "npx http-server ./build -o",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write . && eslint . --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ota-meshi/eslint-plugin-promise-playground.git"
	},
